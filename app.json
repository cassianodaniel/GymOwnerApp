{
  "expo": {
    "name": "GymOwnerApp",
    "slug": "gym-owner-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/gym.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danielchaves.gymownerapp"
    },
    "android": {
      "package": "com.danielchaves.gymownerapp"
    },
    "description": "Gym management system app"
  }
}
